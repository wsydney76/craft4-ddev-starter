{
  "description": "Multilingual Craft CMS starter for use with DDEV",
  "name": "wsydney76/craft4-ddev-starter",
  "version": "3.4.0",
  "license": "MIT",
  "require": {
    "craftcms/cms": "4.5.7",
    "craftcms/contact-form": "3.0.1",
    "craftcms/contact-form-honeypot": "^2.0",
    "fakerphp/faker": "^1.20",
    "nystudio107/craft-closure": "^1.0",
    "nystudio107/craft-code-field": "4.0.11",
    "nystudio107/craft-vite": "4.0.6",
    "putyourlightson/craft-sprig": "2.7.2",
    "vaersaagod/seomate": "2.1.2",
    "vlucas/phpdotenv": "^5.4",
    "wsydney76/craft-contentoverview": "^5.4",
    "wsydney76/elementmap": "^4.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/generator": "^1.3",
    "craftcms/phpstan": "dev-main",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "pre-update-cmd": [
      "@php craft off"
    ],
    "post-update-cmd": [
      "@php craft clear-caches/all",
      "@php craft migrate/all",
      "@php craft on",
      "@php craft project-config/rebuild"
    ],
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
